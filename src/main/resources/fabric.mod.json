{
  "schemaVersion": 1,
  "id": "porting_lib",
  "version": "${version}",
  "name": "Porting Lib",
  "description": "A collection of utilities for porting mods from Forge to Fabric.",
  "authors": [
    "The Create Fabric Team"
  ],
  "contact": {
    "sources": "https://github.com/Fabricators-of-Create/Porting-Lib"
  },
  "license": "LGPL",
  "icon": "assets/porting_lib/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.fabricators_of_create.porting_lib.PortingLib"
    ],
    "client": [
      "io.github.fabricators_of_create.porting_lib.PortingLib"
    ]
  },
  "mixins": [
    "porting_lib.mixins.client.json",
    "porting_lib.mixins.common.json"
  ],
  "accessWidener": "porting_lib.accesswidener",
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": ">=${fabric_version}",
    "minecraft": ">=${minecraft_version}",
    "java": ">=${java_version}"
  },
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/class_293": ["io/github/fabricators_of_create/porting_lib/extensions/VertexFormatExtensions"],
      "net/minecraft/class_276": ["io/github/fabricators_of_create/porting_lib/extensions/RenderTargetExtensions"],
      "net/minecraft/class_1077": ["io/github/fabricators_of_create/porting_lib/extensions/LanguageInfoExtensions"],
      "net/minecraft/class_1159": ["io/github/fabricators_of_create/porting_lib/extensions/Matrix4fExtensions"],
      "net/minecraft/class_1297": [
        "io/github/fabricators_of_create/porting_lib/extensions/EntityExtensions",
        "io/github/fabricators_of_create/porting_lib/extensions/RegistryNameProvider"
      ],
      "net/minecraft/class_1438": ["io/github/fabricators_of_create/porting_lib/extensions/IShearable"],
      "net/minecraft/class_1472": ["io/github/fabricators_of_create/porting_lib/extensions/IShearable"],
      "net/minecraft/class_1473": ["io/github/fabricators_of_create/porting_lib/extensions/IShearable"],
      "net/minecraft/class_1735": ["io/github/fabricators_of_create/porting_lib/extensions/SlotExtensions"],
      "net/minecraft/class_1792": ["io/github/fabricators_of_create/porting_lib/extensions/RegistryNameProvider"],
      "net/minecraft/class_1799": ["io/github/fabricators_of_create/porting_lib/extensions/ItemStackExtensions"],
      "net/minecraft/class_1832": ["io/github/fabricators_of_create/porting_lib/extensions/TierExtensions"],
      "net/minecraft/class_1856": ["io/github/fabricators_of_create/porting_lib/extensions/IngredientExtensions"],
      "net/minecraft/class_2248": ["io/github/fabricators_of_create/porting_lib/extensions/RegistryNameProvider"],
      "net/minecraft/class_2311": ["io/github/fabricators_of_create/porting_lib/extensions/IShearable"],
      "net/minecraft/class_2396": ["io/github/fabricators_of_create/porting_lib/extensions/RegistryNameProvider"],
      "net/minecraft/class_2397": ["io/github/fabricators_of_create/porting_lib/extensions/IShearable"],
      "net/minecraft/class_2476": ["io/github/fabricators_of_create/porting_lib/extensions/IShearable"],
      "net/minecraft/class_2526": ["io/github/fabricators_of_create/porting_lib/extensions/IShearable"],
      "net/minecraft/class_2541": ["io/github/fabricators_of_create/porting_lib/extensions/IShearable"],
