      "net/minecraft/class_2586": ["io/github/fabricators_of_create/porting_lib/extensions/BlockEntityExtensions"],
      "net/minecraft/class_2604": ["io/github/fabricators_of_create/porting_lib/extensions/ClientboundAddEntityPacketExtensions"],
      "net/minecraft/class_2960": ["io/github/fabricators_of_create/porting_lib/extensions/ResourceLocationExtensions"],
      "net/minecraft/class_3611": [
        "io/github/fabricators_of_create/porting_lib/extensions/FluidExtensions",
